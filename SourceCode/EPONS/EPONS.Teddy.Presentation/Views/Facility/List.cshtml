@model EPONS.Teddy.Presentation.ViewObjects.Facility.List
@{
    ViewBag.Title = "List";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-md-10">
        <h2>Facility List</h2>
        <ol class="breadcrumb">
            <li>
                <a href="@Url.Action("","")">Home</a>
            </li>
            <li>
                <a>Facility</a>
            </li>
            <li class="active">
                <strong>List</strong>
            </li>
        </ol>
    </div>
    <div class="col-md-2">

    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-md-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>List of Facilities</h5>
                    <div class="ibox-tools">
                            <a href="@Url.Action("Create", "Facility")" class="btn btn-info btn-sm btn-action">Create Facility</a>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="table-responsive">
                        @Html.Partial("_ListFacilities", Model.Facilities)
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


