@model IEnumerable<EPONS.Teddy.Application.EntityViews.Facility>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                <a href="@Url.Action("Edit", "Facility", new { facilityId = item.Id })" class="btn btn-primary pull-right btn-action">Edit</a>
            </td>
        </tr>
    }

</table>


